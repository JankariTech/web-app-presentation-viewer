{
  "name": "web-app-presentation-viewer",
  "version": "0.0.1",
  "description": "Markdown Presentation Viewer for ownCloud Web",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm vite build",
    "build:w": "pnpm vite build --watch --mode development",
    "lint": "eslint './*.{js,ts}' '{src,tests}/**/*.{js,ts,vue}' --color"
  },
  "dependencies": {
    "@types/reveal.js": "^4.4.8",
    "reveal.js": "^5.0.4"
  },
  "devDependencies": {
    "@ownclouders/extension-sdk": "0.0.5-alpha.2",
    "@ownclouders/prettier-config": "0.0.1",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vue": "^9.22.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue": "^3.4.16"
  },
  "peerDependencies": {
    "@ownclouders/web-client": "0.0.1",
    "@ownclouders/web-pkg": "0.0.5-alpha.9"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "design-system"
      ]
    }
  },
  "packageManager": "pnpm@8.15.1"
}
